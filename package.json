    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.12.1",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
